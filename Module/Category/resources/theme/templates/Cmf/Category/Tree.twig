{% set categories = object.getCategory().getChildren() %}
{% if categories | length %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h4>{{ title }}</h4>
        </div>
        <div class="panel-body">
            {{ render('Cmf/Category/Tree/List', {categoriesTree: object, categories: categories}) }}
        </div>
    </div>
{% endif %}
